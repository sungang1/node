{
  "name": "moaserver",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts",
  "repository": {
    "type": "git",
    "url": " http://192.168.10.4/BoGit/mOA.git"
  },
  "scripts": {
    "start": "nodemon ./lib/index.js",
    "debug": "concurrently \"tsc -w\" \"nodemon ./lib/index.js\"",
    "prod": "tsc && pm2 start ./lib/index.js",
    "update": "git pull && pm2 restart moats",
    "test": "python tasks/test.py",
    "prep": "typings install && copyfiles analytics.js dist && catw node_modules/material-design-lite/material.min.js node_modules/d3/d3.min.js node_modules/seedrandom/seedrandom.min.js > dist/lib.js",
    "build-css": "catw node_modules/material-design-lite/material.min.css styles.css | cleancss > dist/bundle.css",
    "watch-css": "catw node_modules/material-design-lite/material.min.css styles.css -o dist/bundle.css ",
    "build-html": "html-minifier --collapse-whitespace -o dist/index.html index.html",
    "watch-html": "catw index.html -o dist/index.html",
    "build-js": "browserify src/playground.ts -p [tsify] | uglifyjs -c > dist/bundle.js",
    "watch-js": "watchify src/playground.ts -p [tsify] -v --debug -o dist/bundle.js",
    "build": "npm run prep && npm run build-js && npm run build-css && npm run build-html",
    "watch": "npm run prep && concurrently \"npm run watch-js\" \"npm run watch-css\" \"npm run watch-html\"",
    "serve": "http-server dist/",
    "serve-watch": "concurrently \"live-server dist/\" \"npm run watch\""
  },
  "author": "lnit",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.0.35",
    "@types/fs-extra": "^3.0.0",
    "@types/mongoose": "^4.7.10",
    "@types/multiparty": "0.0.31",
    "body-parser": "^1.14.2",
    "clear": "0.0.1",
    "concurrently": "^3.4.0",
    "connect": "^3.4.0",
    "copyfiles": "1.0.0",
    "express": "^4.13.3",
    "gulp": "^3.9.0",
    "gulp-nodemon": "^2.0.6",
    "gulp-strip-debug": "^1.1.0",
    "http-server": "^0.9.0",
    "live-server": "1.1.0",
    "method-override": "^2.3.5",
    "mongojs": "^2.2.2",
    "mongoose": "^4.3.4",
    "nodemon": "^1.19.4",
    "rimraf": "2.5.4",
    "uglify-js": "^2.6.2",
    "watchify": "3.9.0"
  },
  "dependencies": {
    "@types/mysql": "^2.15.7",
    "@types/request-promise": "^4.1.36",
    "@types/ws": "0.0.41",
    "apn": "^2.2.0",
    "co": "^4.6.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.3",
    "ejs": "^2.7.2",
    "excel-export-es6": "^1.0.13",
    "fs-extra": "^3.0.1",
    "log4js": "^6.4.0",
    "multiparty": "^4.1.3",
    "mysql": "^2.17.1",
    "node-rsa": "^0.4.2",
    "nodemon": "^1.19.4",
    "qrcode": "^0.8.1",
    "request": "^2.81.0",
    "request-promise": "^4.2.1",
    "uuid": "^3.0.1",
    "ws": "^2.3.1",
    "xiaomi-push": "^1.0.0"
  }
}
